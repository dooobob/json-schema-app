    "type": "object",
    "properties": {
        "_id": {
            "title": "아이디",
            "type": "string"
        },
        "user_email": {
            "title": "이메일",
            "type": "string"
        },
        "user_pw": {
            "title": "패스워드",
            "type": "string"
        },
        "user_level": {
            "title": "사용자 등급",
            "type": "integer"
        },
        "first_name": {
            "title": "이름",
            "type": "string"
        },
        "last_name": {
            "title": "성",
            "type": "string"
        },
        "nickname": {
            "title": "별명",
            "type": "string"
        },
        "company": {
            "title": "회사/기관",
            "type": "string"
        },
        "department": {
            "title": "부서",
            "type": "string"
        },
        "job_type": {
            "title": "직군",
            "type": "string"
        },
        "created_at": {
