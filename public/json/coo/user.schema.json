            "type": "string"
        },
        "demo": {
            "title": "데모사용자",
            "type": "boolean"
        },
        "notifications": {
            "title": "알림목록",
            "type": "array",
            "items": [{
                "title": "알림",
                "type": "object",
                "properties": {
                    "_id": {
                        "title": "알림 아이디",
                        "type": "string"
