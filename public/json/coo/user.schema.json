    "required": [
        "_id",
        "user_email",
        "user_pw",
        "user_level",
        "first_name",
        "last_name",
        "nickname",
        "company",
        "department",
        "job_type",
        "created_at",
        "updated_at",
        "certificated_at",
        "closed_at",
        "marketing_consent",
        "certification_confirmed",
        "salt",
        "tmp_pw",
        "signup_type",
        "triage_tutorial",
        "oauth_uuid",
        "visit",
        "license",
        "profile_img_path",
        "auth"
    ]
