            "type": "object",
            "properties": {
                "token": {
                    "title": "토큰",
                    "type": "string"
                },
                "updated_at": {
                    "title": "업데이트 일시",
                    "type": "integer"
                }
            },
            "required": [
                "token",
                "updated_at"
            ]
        }
